<div class='twelve columns navbar'>
  <%= link_to root_path do %>
    <h4 class='nav-brand'>Supervisor Contact Book</h4>
  <% end %>
  <ul class='nav-links'>
    <% if signed_in? %>
      <li><%= link_to 'My Contacts', contacts_path %></li>
      <li><%= link_to 'Edit Account', edit_user_registration_path %></li>
